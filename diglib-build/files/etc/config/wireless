
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option txpower '17'
	option hwmode '11g'
	option disabled '0'
	option channel '11'
	option htmode 'HT20'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option isolate '0'
	option maxassoc '35'
	option disabled '0'
	option key 'password'
	option ssid 'Digital-Library'
	option encryption 'psk'

config wifi-iface 'ap_1'
	option device 'radio0'
	option ifname 'wlan0-3'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option maxassoc '5'
	option disabled '1'
	option key 'password2'
	option ssid 'Public-Digital-Library'
	option encryption 'psk'

