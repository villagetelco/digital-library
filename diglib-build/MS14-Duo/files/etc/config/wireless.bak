
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option txpower '17'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option isolate '0'
	option maxassoc '30'
	option ssid 'Digital-Library'
	option encryption 'psk'
	option key 'password'
	option disabled '0'

config wifi-iface 'ap_1'
	option device 'radio0'
	option ifname 'wlan0-3'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option ssid 'Digital-Library-Guest'
	option encryption 'psk'
	option maxassoc '10'
	option key 'passwordguest'
	option disabled '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wwan'
	option encryption 'psk2'
	option key 'Host-Password'
	option ssid 'Host-SSID'
	option disabled '1'

config wifi-iface 'ah_0'                     
	option device 'radio0'               
	option ifname 'wlan0-4'              
	option mode 'mesh'                   
	option mesh_id 'vt-mesh-11s'         
	option network 'wan'                 
	option disabled '1'   
	option encryption 'psk2/aes'
	option key 'meshpassword'


config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'platform/ehci-platform/usb1/1-1/1-1.3/1-1.3:1.0'
	option htmode 'HT20'
	option disabled '0'
	option txpower '15'

config wifi-iface 'ap5_0'
	option device 'radio1'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Digital-Library-5'
	option key 'password'
	option hidden '0'
	option maxassoc '30'
	option isolate '0'
	option encryption 'psk'
	option disabled '0'

config wifi-iface 'ap5_1'
	option device 'radio1'
	option ifname 'wlan1-3'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option encryption 'psk'
	option maxassoc '10'
	option key 'passwordguest'
	option ssid 'Digital-Library-Guest-5'
	option disabled '0'

config wifi-iface 'sta_1'
	option device 'radio1'
	option ifname 'wlan1-2'
	option mode 'sta'
	option network 'wwan'
	option encryption 'psk2'
	option key 'Host-Password'
	option ssid 'Host-SSID'
	option disabled '1'


