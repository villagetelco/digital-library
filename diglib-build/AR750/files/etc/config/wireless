
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy1'
	option txpower '17'
	option hwmode '11g'
	option disabled '0'
	option channel '11'
	option htmode 'HT20'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option isolate '0'
	option maxassoc '35'
	option disabled '0'
	option ssid 'Digital-Library-2'
	option encryption 'psk'
	option key 'password'
	option isolate '0'

config wifi-iface 'ap_2'
	option device 'radio0'
	option ifname 'wlan0-3'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option maxassoc '5'
	option disabled '1'
	option key 'password'
	option ssid 'Public-Digital-Library-Guest'
	option encryption 'psk'
	option isolate '1'
	
config wifi-device 'radio1'
	option type 'mac80211'
	option channel '157'
	option phy 'phy0'
	option hwmode '11a'
	option htmode 'VHT80'
	option disabled '0'
	option country ''
	option coverage '5'
	option txpower '20'
	option band '5G'

config wifi-iface 'ap_1'
	option device 'radio1'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Public-Digital-Library-5'
	option encryption 'psk2'
	option key 'password'
	option hidden '0'
	option maxassoc '30'
	option disabled '0'
	option isolate '0'

