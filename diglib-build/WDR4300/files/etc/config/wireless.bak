
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option txpower '12'
	option band '2g'
	option htmode 'HT40'
	option disabled '0'
	option channel '11'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option isolate '0'
	option maxassoc '30'
	option disabled '0'
	option ssid 'Digital-Library'
	option encryption 'psk'
	option key 'password'

config wifi-iface 'ap_1'
	option device 'radio0'
	option ifname 'wlan0-3'
	option mode 'ap'
	option network 'gst'
	option isolate '1'
	option disabled '0'
	option ssid 'Digital-Library-Guest'
	option encryption 'psk'
	option maxassoc '10'
	option key 'passwordguest'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wwan'
	option encryption 'psk2'
	option ssid 'Host-SSID'
	option key 'Host-Passphrase'
	option disabled '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0'
	option hwmode '11a'
	option disabled '0'
	option coverage '5'
	option band '5G'
	option txpower '15'
	option channel '44'
	option htmode 'HT20'


config wifi-iface 'ap5_0'
	option device 'radio1'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Digital-Library-5'
	option key 'password'
	option hidden '0'
	option maxassoc '30'
	option isolate '0'
	option encryption 'psk'
	option disabled '0'

config wifi-iface 'ap5_1'
	option device 'radio1'
	option ifname 'wlan1-3'
	option mode 'ap'
	option network 'gst'
	option isolate '1'
	option encryption 'psk'
	option maxassoc '10'
	option key 'passwordguest'
	option ssid 'Digital-Library-Guest-5'
	option disabled '0'

config wifi-iface 'sta_1'
	option device 'radio1'
	option ifname 'wlan1-2'
	option mode 'sta'
	option network 'wwan'
	option encryption 'psk2'
	option ssid 'Host-SSID'
	option key 'Host-Passphrase'
	option disabled '1'

config wifi-iface 'ah_0'
	option device 'radio1'
	option ifname 'wlan0-4'
	option mode 'mesh'
	option mesh_id 'vt-mesh-11s'
	option network 'wan'
	option disabled '1'
	option encryption 'psk2/aes'
	option key 'meshpassword'
